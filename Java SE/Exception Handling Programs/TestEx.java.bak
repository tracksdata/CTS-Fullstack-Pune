import java.util.*;
public class TestEx{
public static void main(String[] args){
		int x=1;
		Scanner sc=new Scanner(System.in);
		do{
		try{
			System.out.print("First No: ");
			int a=sc.nextInt();
			System.out.print("Second No: ");
			int b=sc.nextInt();
			int c=a/b;
			System.out.println("Division is "+c);
			x=2;
		}catch(Exception e){
		System.out.println("That is Not Possible");
		}
		}while(x==1);

	}
}
