public class EXDemo1{
public static void main(String[] args){
		int x=1;
		do{
		try{
			int a=Integer.parseInt(args[0]);
			int b=Integer.parseInt(args[1]);
			int c=a/b;
			System.out.println("Division is "+c);
			x=2;
		}catch(Exception e){
		System.out.println("That is Not Possible");
		}
		}while(x==1);

	}
}
